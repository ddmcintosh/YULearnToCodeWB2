// sample input 
var savings = 100000; 
var interest = 8.00;
var rule = 72;
var year = 9; 
var newBalance = savings * 2; 



// calculation 
var years = rule / interest;


// output 
console.log ("At a " + interest + " your savings account will be worth " + newBalance.toFixed(2) + " in " + years.toFixed(1) + " year ");









